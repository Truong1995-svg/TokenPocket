.basic-menu-item { color: var(--vscode-menu-foreground); background-color: transparent; cursor: pointer; border: none; user-select: none; padding: 4px 16px; display: flex; align-items: center; flex-direction: row; flex: 1 1 0%; font-family: var(--vscode-font-family); font-size: 13px; line-height: 18px; font-weight: 500; outline: 0px; border-radius: 3px; margin: 0px 4px; }

