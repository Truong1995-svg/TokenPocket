.basic-menu { border-radius: 4px; display: inline-flex; flex-direction: column; min-width: 180px; max-width: 280px; background: var(--vscode-menu-background,#000); padding: 4px 0px; border: 1px solid var(--vscode-menu-border); box-shadow: 0 2px 8px var(--vscode-widget-shadow); animation: 0.15s cubic-bezier(0.4, 0, 0.2, 1) 0s 1 normal none running popup-menu-enter; }

