body { background-color: var(--vscode-panel-background); position: relative; min-height: 100vh; --body-padding: 40px; padding: 0px 40px; }

@media (max-width: 431px) {
  body { --body-padding: 24px; padding: 0px 24px; }
}

body:not(.capra)::after { content: ""; background-image: radial-gradient(circle,var(--vscode-panel-border) 1.5px,transparent 0); opacity: 0.5; background-size: 48px 64px; background-position: 50% 0px; width: 100%; margin-right: auto; margin-left: auto; position: absolute; inset: 0px; z-index: -10; }

.banner:not(:empty) { margin-top: 24px; margin-right: 16px; }

.shine { position: absolute; inset: 0px; z-index: -10; background-image: radial-gradient(500px 200px at 50% 0px, rgb(19, 31, 51) 20%, rgba(19, 31, 51, 0) 100%); pointer-events: none; }

@media (prefers-color-scheme: light) {
  :host-context(body:not(.dark-theme)) .shine { background-image: radial-gradient(500px 200px at 50% 0px, rgb(255, 255, 255) 20%, rgba(255, 255, 255, 0) 100%); }
  body:not(.dark-theme) .shine { background-image: radial-gradient(500px 200px at 50% 0px, rgb(255, 255, 255) 20%, rgba(255, 255, 255, 0) 100%); }
}

:host-context(body.light-theme) .shine { background-image: radial-gradient(500px 200px at 50% 0px, rgb(255, 255, 255) 20%, rgba(255, 255, 255, 0) 100%); }

body.light-theme .shine { background-image: radial-gradient(500px 200px at 50% 0px, rgb(255, 255, 255) 20%, rgba(255, 255, 255, 0) 100%); }

body.capra .shine { background-image: none; }

